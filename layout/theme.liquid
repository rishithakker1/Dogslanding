    {% include 'kiwiSizing' %}
    
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1">

    <!-- Preconnect Domains -->
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
    <link rel="preconnect" href="https://fonts.shopify.com" crossorigin>
    <link rel="preconnect" href="https://monorail-edge.shopifysvc.com">

    <!-- Preload Custom Fonts -->
    <link rel="preload" href="{{ "THICCCBOI-Black.woff2" | asset_url }}" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Black.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Bold.woff2" | asset_url }}" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Bold.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-ExtraBold.woff2" | asset_url }}" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-ExtraBold.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Light.woff2" | asset_url }}" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Light.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Medium.woff2" | asset_url }}" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Medium.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Regular.woff2" | asset_url }}" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Regular.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-SemiBold.woff2" | asset_url }}" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-SemiBold.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-ThicccAF.woff2" | asset_url }}" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-ThicccAF.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Thin.woff2" | asset_url }}" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="{{ "THICCCBOI-Thin.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>
    <link rel="preload" href="{{ "DancingScript-Regular.ttf" | asset_url }}" as="font" type="font/ttf" crossorigin>

    

    <!-- Preload TinySlider Assets -->
    <link rel="preload" href="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.3/tiny-slider.css" as="style">
    <link rel="preload" href="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.2/min/tiny-slider.js" as="script">

    <!-- Preload Assets -->
    <link rel="preload" href="{{ 'theme.css' | asset_url }}" as="style">
    {{ 'hup.css' | asset_url | stylesheet_tag }}
    <link rel="preload" href="{{ 'atlantic.js' | asset_url }}" as="script">
    <link rel="preload" href="{{ 'plugins.js' | asset_url }}" as="script">
    <link rel="preload" href="{{ 'api.jquery.js' | asset_url }}" as="script">
    <link rel="preload" href="{{ 'atlantic-icons.woff' | asset_url }}" as="font" type="font/woff" crossorigin>
    <link rel="preload" href="{{ 'custom.js' | asset_url }}" as="script">

    {% if template contains 'product' %}
    <link rel="preload" href="{{ 'ajaxify_cart.js' | asset_url }}" as="script">
    {% endif %}

    {% if template contains 'cart' %}
    <link rel="preload" href="{{ '/services/javascripts/countries.js' }}" as="script">
    <link rel="preload" href="{{ 'shopify_common.js' | shopify_asset_url }}" as="script">
    <link rel="preload" href="{{ 'jquery.cart.min.js' | asset_url }}" as="script">
    {% endif %}

    {% if template contains 'customers' %}
    <link rel="preload" href="{{ 'shopify_common.js' | shopify_asset_url }}" as="script">
    <link rel="preload" href="{{ 'customer_area.js' | shopify_asset_url }}" as="script">
    {% endif %}

    <title>
      {{- page_title -}}

      {%- if current_tags -%}
      &ndash; "{{ current_tags | join: ', ' }}"
      {%- endif -%}

      {%- if current_page != 1 -%}
      &ndash; {{ 'general.pagination.seo_title' | t }} {{ current_page }}
      {%- endif -%}

      {%- unless page_title contains shop.name -%}
      &ndash; {{ shop.name }}
      {%- endunless -%}
    </title>

    {% if page_description or shop.description != blank %}
    <meta name="description" content="{% if page_description %}{{ page_description }}{% else %}{{ shop.description }}{% endif %}">
    {% endif %}

    {% render 'favicon' %}

    {% if template contains 'collection' and current_tags %}
    <meta name="robots" content="noindex" />
    <link rel="canonical" href="{{ shop.url }}{{ collection.url }}" />
    {% else %}
    <link rel="canonical" href="{{ canonical_url }}" />
    {% endif %}

    {{ content_for_header }}

    {% assign twitter_handle = settings.social_twitter_url | split: 'twitter.com/' | last %}
    {%
    render 'social-meta-tags',
    twitter_handle: twitter_handle
    %}

    <script>
      document.documentElement.className=document.documentElement.className.replace(/\bno-js\b/,'js');
      if(window.Shopify&&window.Shopify.designMode)document.documentElement.className+=' in-theme-editor';
      if(('ontouchstart' in window)||window.DocumentTouch&&document instanceof DocumentTouch)document.documentElement.className=document.documentElement.className.replace(/\bno-touch\b/,'has-touch');
    </script>

    <!-- TinySlider CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.3/tiny-slider.css">
    <!-- Theme CSS -->
    <link rel="stylesheet" href="{{ 'theme.css' | asset_url }}">


    <!-- Store object -->
    {% render 'theme-options' %}

    {{ shop.metafields.loox["global_html_head"] }}
  
  {% render 'shogun-head' %}
{% include 'dexter' %}

    <script type="text/javascript" src="https://vaultcdn.electricapps.net/lib/jquery-ui-min.js?shop=skinmag.myshopify.com" async="async"></script>
{% include "omega_multi_pixel" %} </head>

<body
  class="
    {% if template contains 'contact' %}
      template-contact
    {% else %}
      template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}
    {% endif %}
    {% if settings.meta-font == "'Open Sans' sans-serif" %}
      font-open-sans
    {% endif %}
    {% if settings.main-title-uppercase %}
      uppercase
    {% endif %}
    product-grid-{{ settings.product_grid_aspect_ratio }}
  "
  >

  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-TC5DMXS"
                    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->
  
  <div id="fb-root"></div>
  <script>(function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=187795038002910";
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));</script>

  {% render 'photoswipe' %}

  {% render 'quickshop' %}
  {% comment %}Inject sidebar-drawer begin{% endcomment %}
  <div class="sidebar-drawer-container" data-sidebar-drawer-container>
    <div class="sidebar-drawer" data-sidebar-drawer tab-index="-1">
      <div class="sidebar-drawer__header-container">
        <div class="sidebar-drawer__header" data-sidebar-drawer-header></div>
  
        <button
          class="sidebar-drawer__header-close"
          aria-label="close"
          data-sidebar-drawer-close
        >
          {% render 'icon' with 'close-alt' %}
        </button>
      </div>
  
      <div class="sidebar-drawer__content" data-sidebar-drawer-content></div>
    </div>
  </div>
  {% comment %}Inject sidebar-drawer end{% endcomment %}

  {% section 'pxs-announcement-bar' %}
  {% section 'static-header' %}

  <div class="page-body-content">
    {{ content_for_layout }}
  </div>

  {% section 'static-footer' %}

  <!-- Scripts -->
  
  <script src="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.2/min/tiny-slider.js"></script>
  
  <script
    src="{{ 'atlantic.js' | asset_url }}"
    data-scripts
    data-shopify-api-url="{{ 'api.jquery.js' | asset_url }}"
    data-shopify-countries="/services/javascripts/countries.js"
    data-shopify-common="{{ 'shopify_common.js' | shopify_asset_url }}"
    data-shopify-cart="{{ 'jquery.cart.min.js' | asset_url }}"
    data-customer-area="{{ 'customer_area.js'  | shopify_asset_url }}"
    defer>
  </script>

  <script src="{{ 'api.jquery.js' | asset_url }}" defer></script>
  <script src="{{ 'plugins.js' | asset_url }}" defer></script>
  <script src="{{ 'custom.js' | asset_url }}" defer></script>
 
  {% if template contains 'cart' %}
    <script src="{{ '/services/javascripts/countries.js' }}" defer></script>
    <script src="{{ "shopify_common.js" | shopify_asset_url }}" defer></script>
    <script src="{{ 'jquery.cart.min.js' | asset_url }}" defer></script>
  {% endif %}

  {% if template contains 'customers' %}
    <script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer></script>
    <script src="{{ 'customer_area.js'  | shopify_asset_url }}" defer></script>
  {% endif %}

  {% render 'structured-data' %}

  <script>
    (function () {
      function handleFirstTab(e) {
        if (e.keyCode === 9) { // the "I am a keyboard user" key
          document.body.classList.add('user-is-tabbing');
          window.removeEventListener('keydown', handleFirstTab);
        }
      }
      window.addEventListener('keydown', handleFirstTab);
    })();
  </script>

  {% if template contains "product" %}
  <script src="{{ 'ajaxify_cart.js' | asset_url }}" defer></script>
  {% endif %}

  {{ shop.metafields.loox["global_html_body"] }}

  {% render 'preorder-now' %}

  <script>
    var store_template = {{ template | json }};
  </script>
  
  {% if template == "index" %}
  <script type="text/javascript" src="//www.klaviyo.com/media/js/public/klaviyo_subscribe.js" defer></script>
  {% endif %}
  






</body>
</html>